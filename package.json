{
  "name": "magx-examples",
  "version": "0.2.3",
  "description": "Examples of how to use magx, mosx and magx-client",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "cluster": "ts-node src/cluster.ts",
    "pm2": "tsc && pm2 start pm2.config.js",
    "serve": "nodemon --watch '**/*.ts' --ignore '**/*.spec.ts' --exec ts-node src/index.ts"
  },
  "keywords": [
    "magx",
    "mosx",
    "examples"
  ],
  "author": "Damir Yusipov",
  "license": "MIT",
  "dependencies": {
    "ioredis": "^4.27.6",
    "magx": "^0.5.2",
    "magx-client": "^0.7.2",
    "magx-monitor": "0.0.7",
    "mongodb": "^4.0.1",
    "mosx": "^0.8.8"
  },
  "devDependencies": {
    "@types/ioredis": "^4.26.6",
    "@types/node": "^11.15.54",
    "nodemon": "^1.19.4",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.10"
  }
}
