{
  "name": "magx-examples",
  "private": true,
  "version": "0.2.5",
  "description": "Examples of how to use magx, mosx and magx-client",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "cluster": "ts-node src/cluster.ts",
    "pm2": "tsc && pm2 start pm2.config.js",
    "serve": "nodemon --watch '**/*.ts' --ignore '**/*.spec.ts' --exec ts-node src/index.ts"
  },
  "keywords": [
    "magx",
    "mosx",
    "examples"
  ],
  "author": "Damir Yusipov",
  "license": "MIT",
  "dependencies": {
    "ioredis": "^4.28.2",
    "magx": "^0.5.13",
    "magx-client": "^0.7.2",
    "magx-monitor": "0.0.7",
    "mongodb": "^4.2.2",
    "mosx": "^0.8.11"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.6",
    "@types/node": "^17.0.7",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  }
}
